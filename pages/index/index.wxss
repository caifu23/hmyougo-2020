/**index.wxss**/

.search {
  padding: 20rpx;
  background-color: #ff2d4a;
}
.search-in {
  display: flex;
  justify-content: center;
  align-items: middle;
  border-radius: 4px;
  padding: 10rpx 0;
  font-size: 14px;
  color: #bbb;
  background-color: #fff;
}
.search-in icon {
  margin-right: 15rpx;
}

.swiper {
  height: 340rpx;
}
.swiper image {
  width: 750rpx;
  height: 340rpx;
}

.navMenu {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 24rpx 0;
}
.navMenu navigator {
  width: 25%;
  flex: 1;
  text-align: center;
}
.navMenu navigator image {
  width: 128rpx;
  height: 140rpx;
}